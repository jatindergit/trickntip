msgstr "désactive l'annonce au milieu de l'article ou de la page"

#: views/templates/info.php:29
msgid "suppresses the ad before the last paragraph"
msgstr "désactive l'annonce avant le dernier paragraphe"

#: views/templates/info.php:30
msgid "suppresses the ad below the post or page content"
msgstr "désactive l'annonce sous le contenu de l'article ou de la page"

#: views/templates/info.php:31
